{
  "name": "watch-all-gh-org-repos",
  "version": "0.2.0",
  "description": "CLI to watch all GitHub organization's repos",
  "bin": {
    "wagor": "bin/wagor"
  },
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "test": "npm run build && cross-env BABEL_ENV=commonjs mocha --compilers js:babel-register --recursive",
    "cover": "cross-env BABEL_ENV=commonjs istanbul cover _mocha -- --compilers js:babel-register --recursive",
    "lint": "eslint src test",
    "build": "npm run clean && cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "pre-publish": "npm run clean && npm run lint && npm run test && npm run build",
    "preversion": "npm run clean && npm run lint && npm test",
    "version": "npm run build && git add -A .",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gedex/watch-all-gh-org-repos.git"
  },
  "author": "Akeda Bagus <admin@gedex.web.id>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gedex/watch-all-gh-org-repos/issues"
  },
  "homepage": "https://github.com/gedex/watch-all-gh-org-repos#readme",
  "dependencies": {
    "bluebird": "^3.4.7",
    "chalk": "^1.1.3",
    "cli-table": "^0.3.1",
    "commander": "2.9.0",
    "github": "^9.0.0",
    "randomstring": "^1.1.5",
    "read": "^1.0.7"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.22.0",
    "chai": "^3.5.0",
    "cross-env": "^3.0.0",
    "eslint": "^3.11.1",
    "eslint-config-wpcalypso": "^0.6.0",
    "eslint-plugin-wpcalypso": "^3.0.2",
    "istanbul": "^1.0.0-alpha",
    "mocha": "^3.2.0",
    "sinon": "^1.17.7"
  }
}
